{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AzureAdB2C": {
    "Instance": "https://<your tenant>.b2clogin.com/tfp/",
    "ClientId": "<clientid for this api. sample's 'your api'>",
    "Domain": "<your tenant>.onmicrosoft.com",
    "SignUpSignInPolicyId": "B2C_1_<your policy>"
  },
  "NswagSettings": {
    "UseSwaggerUi3": {
      "ClientId": "<swagger-ui-client Id>",
      "AppName": "swagger-ui-client"
    },
    "Security": {
      "AuthorizationUrl": "https://<your tenant>.b2clogin.com/<your tenant>.onmicrosoft.com/oauth2/v2.0/authorize?p=B2C_1_<your policy>",
      "TokenUrl": "https://<your tenant>.b2clogin.com/<your tenant>.onmicrosoft.com/oauth2/v2.0/token?p=B2C_1_<your policy>",
      "ScopePath": "https://<your tenant>.onmicrosoft.com/<sample's 'your api' name>/user_impersonation",
      "ScopeDescription": "Access the api as the signed-in user"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationCommandDbConnection": "Data Source=localhost;Initial Catalog=locatDatabase;User ID=SA;Password=;",
    "ApplicationQueryDbConnection": "Data Source=localhost;Initial Catalog=locatDatabase;User ID=SA;Password=;"
  }
}
